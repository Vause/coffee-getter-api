{
    "env": {
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len": [
            1,
            {
                "code": 160
            }
        ],
        "import/prefer-default-export": "off",
        "import/extensions": "off"
    }
}